# Datenbank (PostgreSQL für Vercel/Neon)
# Lokal: Neon-URL oder z.B. postgresql://user:pass@localhost:5432/estait
DATABASE_URL="postgresql://user:password@host/database?sslmode=require"

# NextAuth (für Produktion erforderlich)
NEXTAUTH_URL="https://deine-domain.vercel.app"
NEXTAUTH_SECRET="generiere-mit-openssl-rand-base64-32"

# Admin-Login (Credentials)
ADMIN_EMAIL="admin@beispiel.de"
ADMIN_PASSWORD="sicheres-passwort"
